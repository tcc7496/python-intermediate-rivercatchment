    "import math\n",
    "\n",
    "class Circle:\n",
    "  def __init__(self, radius):\n",
    "    self.radius = radius\n",
    "  def get_area(self):\n",
    "    return math.pi * self.radius * self.radius\n",
    "\n",
    "my_circle = Circle(10)"
   ]
